{
  "manifest_version": 3,
  "name": "Chrome Bookmark List",
  "version": "2.0.0",
  "description": "新しいタブでブックマークをフォルダ別に表示し、履歴も検索できる拡張機能",
  "permissions": [
    "bookmarks",
    "history",
    "storage"
  ],
  "optional_host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
